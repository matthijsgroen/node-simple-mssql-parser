{
  "name": "node-simple-mssql-parser",
  "version": "1.0.0",
  "description": "A simple parser for MSSQL queries in Node.js",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
